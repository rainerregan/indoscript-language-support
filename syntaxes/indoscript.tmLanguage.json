{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "IndoScript",
	"patterns": [
    {
      "match": "\\b(atur|tampilkan|fungsi|kembali)\\b",
      "name": "keyword.control.indoscript"
    },
    {
      "match": "\\b(jika|selagi|lainnya)\\b",
      "name": "keyword.control.flow.indoscript"
    },
    {
      "match": "\\b(dan|atau|bukan)\\b",
      "name": "keyword.operator.logical.indoscript"
    },
    {
      "match": "\\b(true|false)\\b",
      "name": "constant.language.boolean.indoscript"
    },
    {
      "match": "\".*?\"",
      "name": "string.quoted.double.indoscript"
    },
    {
      "match": "\\b[0-9]+\\b",
      "name": "constant.numeric.indoscript"
    },
    {
      "match": "\\b[a-zA-Z_][a-zA-Z0-9_]*\\b",
      "name": "variable.other.indoscript"
    },
    {
      "match": "//.*$",
      "name": "comment.line.double-slash.indoscript"
    }
  ],
	"repository": {},
	"scopeName": "source.is"
}